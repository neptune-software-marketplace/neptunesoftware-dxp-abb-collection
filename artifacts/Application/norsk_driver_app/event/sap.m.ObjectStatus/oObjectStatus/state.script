if (typeof Status === "undefined" || Status === null || Status === "") { return; }  
var formattedText = Status; 

if(Status === "Assigned"){
    formattedText = "Warning";
}
if(Status === "Unassigned"){
    formattedText = "Error";
}
if(Status === "Completed"){
    formattedText = "Success";
}

return formattedText;